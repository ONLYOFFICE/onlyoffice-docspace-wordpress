{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "onlyoffice-docspace/onlyoffice-docspace",
	"title": "ONLYOFFICE DocSpace",
	"category": "media",
	"editorScript": "file:build/index.js",
	"editorStyle": "file:build/index.css",
	"keywords": ["onlyoffice", "docspace"],
	"textdomain": "onlyoffice-docspace-plugin",
	"attributes": {
		"fileId": {
			"type": "number"
		},
		"fileName": {
			"type": "string"
		},
		"icon":  {
			"type": "string"
		},
		"width":  {
			"type": "string"
		},
		"height":{
			"type": "string"
		},
		"showHeader": {
			"type": "boolean"
		},
		"showTitle": {
			"type": "boolean"
		},
		"showMenu": {
			"type": "boolean"
		},
		"showFilter": {
			"type": "boolean"
		},
		"showAction": {
			"type": "boolean"
		}
	}
}
